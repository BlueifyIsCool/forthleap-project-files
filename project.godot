[application]

config/name="Forthleap"
run/main_scene="res://resource/Scenes/UI/main_menu_gui.tscn"
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://icon.png"

[autoload]

